{
  "name": "ts-web-app-starter",
  "version": "0.1.0",
  "description": "Starter project for a Typescript Web application.",
  "main": "main.js",
  "scripts": {
    "build:dev": "webpack --config config/webpack/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:coverage": "npm run rimraf -- coverage",
    "clean:dist": "npm run rimraf -- dist",
    "clean:docs": "npm run rimraf -- docs",
    "clean:install": "npm run rimraf -- node_modules",
    "clean": "npm run clean:coverage && npm run clean:dist && npm run clean:docs && npm run clean:install",
    "coverage": "npm test && nyc report --reporter=lcov",
    "eslint": "eslint",
    "jest": "jest --verbose --config=jest.config.json",
    "lint": "npm run tslint -- \"src/**/*.ts\" && npm run eslint -- \"config/**\"",
    "postinstall": "npm audit && npm run jest",
    "postversion": "git push && git push --tags && npm run clean:dist",
    "preversion": "npm test",
    "rimraf": "rimraf",
    "start:dev": "webpack-dev-server --open --inline --hot --quiet --config config/webpack/webpack.dev.js",
    "start": "npm run start:dev",
    "test": "npm run lint && npm audit && npm run jest && open-cli coverage/lcov-report/index.html",
    "tslint": "tslint",
    "version": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoic-dev/ts-web-app-starter.git"
  },
  "keywords": [
    "Node",
    "Typescript",
    "web",
    "template",
    "starter",
    "PWA"
  ],
  "author": "John Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoic-dev/ts-web-app-starter/issues"
  },
  "homepage": "https://github.com/stoic-dev/ts-web-app-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^24.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-parameter-decorator": "^1.0.15",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "^6.8.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "open": "^7.0.0",
    "open-cli": "^5.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.2",
    "ts-jest": "^24.3.0",
    "ts-mockito": "^2.5.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-lighthouse-plugin": "^1.0.5",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
